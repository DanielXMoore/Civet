{
  "compilerOptions": {
    "baseUrl": ".",
    "isolatedModules": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "noEmit": false,
    "checkJs": false,
    "declarationMap": false,
    "skipLibCheck": true,
    "target": "ESNext",
    "allowJs": true,
    "jsx": "preserve",
    "emitDeclarationOnly": true,
    "declaration": true
  }
}
