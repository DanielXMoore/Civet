{testCase} from ../helper.civet

describe "[TS] non null assertion", ->
  testCase """
    !. accessor
    ---
    a!.x = y
    ---
    a!.x = y
  """

  testCase """
    index accessor
    ---
    a![x] = y
    ---
    a![x] = y
  """

  testCase """
    expression assertion
    ---
    a := y! + x!
    ---
    const a = y! + x!
  """

  testCase """
    function application
    ---
    a := f!()
    ---
    const a = f!()
  """
