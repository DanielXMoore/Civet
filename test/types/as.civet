{testCase} from ../helper.civet

describe "[TS] as", ->
  testCase """
    type as
    ---
    var x = "yo" as B
    ---
    var x = "yo" as B
  """

  testCase """
    multiple as
    ---
    const results = [] as unknown as T
    ---
    const results = [] as unknown as T
  """

  testCase """
    as! T
    ---
    x as! T
    ---
    x as unknown as T
  """

  testCase """
    as tuple
    ---
    [1, 2] as tuple
    ---
    [1, 2] satisfies readonly unknown[] | []
  """

  testCase """
    as on next line
    ---
    [1, 2]
    as [number, number]
    ---
    [1, 2] as [number, number]
  """

  testCase """
    as indented on next line
    ---
    [1, 2]
      as [number, number]
    ---
    [1, 2] as [number, number]
  """

  testCase """
    with implicit function call
    ---
    "Hello, world!"
    .split /\s+/
    as [string, string]
    ---
    "Hello, world!"
    .split(/\s+/) as [string, string]
  """
