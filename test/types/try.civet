{testCase} from ../helper.civet

describe "[TS] try", ->
  testCase """
    typed catch
    ---
    try
      foo()
    catch e: any
      bar
    ---
    try {
      foo()
    }
    catch (e: any) {
      bar
    }
  """

  // #1445
  testCase """
    typed catch followed by function call
    ---
    try
      foo()
    catch e: any
      bar(e)
    ---
    try {
      foo()
    }
    catch (e: any) {
      bar(e)
    }
  """
