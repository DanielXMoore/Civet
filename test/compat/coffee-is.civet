{testCase} from ../helper.civet

describe "coffeeIs", ->
  testCase """
    converts is to ===
    ---
    "civet coffee-compat"
    a is b
    ---
    a === b
  """

  testCase """
    converts is not to === !
    ---
    "civet coffee-compat"
    a is not b
    ---
    a === !b
  """

  testCase """
    converts is not to !==
    ---
    "civet coffee-is"
    a is not b
    ---
    a !== b
  """
