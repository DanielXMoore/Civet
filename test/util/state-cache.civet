StateCache from ../../source/state-cache.mts
assert from assert

describe "StateCache", ->
  it "should cache based on an array key", ->
    value := ["hi", {}]

    cache := new StateCache<typeof value>

    cache.set ["Rule", 2, 3, ""], value
    got := cache.get ["Rule", 2, 3, ""]

    assert.equal value, got, "get / set"
