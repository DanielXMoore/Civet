              type: "CallExpression",
              children: [base, ...rest.slice(0, -1), {
                type: "PropertyAccess",
                children: [last.dot, {
                  ...v,
                  children: [ v.children.slice(0, 2) ] // Remove potential delimiter
                }],
              }],
            })

            return makeGetterMethod(name, ws, exp, returnType, block, kind)
          })
