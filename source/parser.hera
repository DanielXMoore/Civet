  __:ws ( DotDotDot __ )?:dots1 IdentifierName:name ( _? DotDotDot )?:dots2 (__ (QuestionMark _?)? Colon __):colon Type:type ->
    let dots = dots1 || (dots2 && [dots2[1], dots2[0]] /* space at end */)
    if (dots1 && dots2) {
      dots = [dots, {
        type: "Error",
        message: "... both before and after identifier",
      }]
