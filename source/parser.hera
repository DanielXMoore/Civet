    if (!list.length) return $skip
    // Prepend bullet to first element and append delimiter to last element
    list = list.slice()
    list[0] = prepend(bullet, list[0])
    if (delimiter) {
      const last = list.length - 1
      list[last] = append(list[last], delimiter)
    }
    return list

